{
  "name": "snippetgenerator",
  "displayName": "snippetgenerator",
  "description": "",
  "publisher": "test",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.53.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:generateSnippet"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "generateSnippet",
        "title": "Create a new uilib snippet"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.53.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "mocha": "^8.2.1",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0",
    "webpack": "^5.22.0",
    "webpack-cli": "^4.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/wix/react-native-ui-lib.git",
    "directory": "snippetGenerator"
  }
}
